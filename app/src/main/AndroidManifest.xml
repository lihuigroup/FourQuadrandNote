<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.app.fourquadrantnote">

    <application
        android:label="@string/app_name"
        android:theme="@style/notitle">
        <activity android:name="com.android.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.app.NewNoteActivity" />
        <activity android:name="com.android.app.FourQuadrandActivity" />
        <activity android:name="com.android.app.NoteActivity" />
        <activity android:name="com.android.app.SetActivity" />
        <activity android:name="com.android.app.LoginActivity" />
        <activity android:name="com.android.app.RegisterActivity" />
        <activity android:name="com.android.app.ChangPasswordActivity"></activity>
    </application>

</manifest>